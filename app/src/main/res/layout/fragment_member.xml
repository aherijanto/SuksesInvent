    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.membership.MemberFragment">

    <ImageView
        android:id="@+id/barcodeimageview"
        android:layout_width="match_parent"
        android:layout_height="82dp"
        android:layout_marginTop="80dp"
